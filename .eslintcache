se(JSON.stringify(board))\r\n    const { lists } = boardClone\r\n\r\n    if (type === 'LIST') {\r\n      const [list] = lists.splice(sourceListIdx, 1)\r\n      targetListIdx >= 0 ? lists.splice(targetListIdx, 0, list) : lists.push(list)\r\n    } else if (type === 'CARD') {\r\n      const sourceList = lists[sourceListIdx].cards\r\n      const targetList = lists[targetListIdx].cards\r\n      const [card] = sourceList.splice(sourceCardIdx, 1)\r\n      targetCardIdx >= 0 ? targetList.splice(targetCardIdx, 0, card) : targetList.unshift(card)\r\n    }\r\n    dispatch(updateBoard(boardClone))\r\n  }\r\n\r\n  return (\r\n    <main ref={drop} className={`board flex col as`}>\r\n      <span>{board?.title}</span>\r\n      <section className=\"container flex as\">\r\n        {board?.lists.map((list, idx) => (\r\n          <List list={list} key={list._id} thisListIdx={idx} handleDrop={handleDrop} />\r\n        ))}\r\n      </section>\r\n    </main>\r\n  )\r\n}\r\n","C:\\_DEV\\Projects\\Tallo\\frontEnd\\src\\components\\Header\\Header.jsx",[],"C:\\_DEV\\Projects\\Tallo\\frontEnd\\src\\pages\\Home\\Home.jsx",[],"C:\\_DEV\\Projects\\Tallo\\frontEnd\\src\\store\\board\\BoardReducer.js",[],"C:\\_DEV\\Projects\\Tallo\\frontEnd\\src\\store\\board\\BoardActions.js",[],"C:\\_DEV\\Projects\\Tallo\\frontEnd\\src\\components\\Board\\List\\List.jsx",[],"C:\\_DEV\\Projects\\Tallo\\frontEnd\\src\\service\\BoardService.js",[],"C:\\_DEV\\Projects\\Tallo\\frontEnd\\src\\components\\Board\\Card\\Card.jsx",[],"C:\\_DEV\\Projects\\Tallo\\frontEnd\\src\\service\\HttpService.js",[],{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"143","replacedBy":"147"},{"ruleId":"145","replacedBy":"148"},{"ruleId":"149","severity":1,"message":"150","line":15,"column":12,"nodeType":"151","messageId":"152","endLine":15,"endColumn":18},"no-native-reassign",["153"],"no-negated-in-lhs",["154"],["153"],["154"],"no-unused-vars","'isOver' is assigned a value but never used.","Identifier","u